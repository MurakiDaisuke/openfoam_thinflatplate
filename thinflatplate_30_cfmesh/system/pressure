pressure
{
    type            pressure;
    libs            (fieldFunctionObjects);
    writeControl    onEnd;
    mode            staticCoeff;

    p               p;
    U               U;
    rho             rhoInf;
    rhoInf          1.0;
    pInf            0.0;
    UInf            (1.0 0 0);
}


